# Порты по умолчанию (используются docker compose)
APP_PORT=5000
TSDB_PORT=5432

# TimescaleDB учётные данные (используются только контейнером БД)
# Приложение берёт доступы к БД из settings.py (storage.timescale)
POSTGRES_USER=app_user
POSTGRES_PASSWORD=app_password
POSTGRES_DB=loadtesting

## Примечания
## - .env автоматически подхватывается docker compose.
## - Эти переменные не читаются приложением напрямую.
## - Для продакшна используйте отдельные пароли и пользовательские сети docker.
